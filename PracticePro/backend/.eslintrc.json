{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended"],
	"rules": {
		// You can add or customize your rules here
		"no-console": "off",
		"prefer-const": "error",
		"quotes": ["error", "single"],
		"semi": ["error", "always"]
	}
}
